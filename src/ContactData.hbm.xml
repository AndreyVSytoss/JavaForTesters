<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
 <class name="com.example.tests.ContactData" table="addressbook">
 <id name="id" column="id" ></id>
 <property name="firstname" column="lastname" />
 <property name="lastname" column="firstname" />
 <property name="address" column="address" />
 <property name="homenumb" column="home" />
 <property name="mobilenumb" column="mobile" />
 <property name="worknumb" column="work" />
 <property name="email" column="email" />
 <property name="email2" column="email2" />
 <property name="bday" column="bday" />
 <property name="bmonth" column="bmonth" />
 <property name="byear" column="byear" />
 <property name="address2" column="address2" />
 <property name="secondpfone" column="phone2" />
 </class>
</hibernate-mapping>

